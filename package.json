{
  "name": "ebay-sandbox-app",
  "version": "1.0.0",
  "description": "A ebay sandbox app based on angular2",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server -c bs-config.js\"",
    "e2e": "tsc && concurrently \"http-server -s\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Narayanan Ganapathi",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.2",
    "systemjs": "^0.19.41",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "lite-server": "^2.2.2",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4"
  }
}
